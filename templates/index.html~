<script type="text/javascript">
function accept(){
 	    var username=$('#username').val();
	    var password=$('#password').val();
    	if(username==''||password=='')
  	{ $('#error-msg').html('error');
	  return false; 
    	
	}
	else { return true; }
	
 }	
	
</script>
